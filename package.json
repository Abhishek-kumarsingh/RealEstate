{
  "name": "nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "rm -rf node_modules/.prisma && prisma generate && next build",
    "build:debug": "prisma generate && next build --debug",
    "build:no-typecheck": "prisma generate && next build --no-type-check",
    "build:simple": "next build",
    "build:vercel": "prisma generate && prisma db push --accept-data-loss && next build",
    "start": "next start",
    "lint": "next lint",
    "postinstall": "node scripts/prisma-generate-windows.js",
    "seed": "tsx scripts/seed.ts",
    "seed-prisma": "tsx scripts/seed-prisma.ts",
    "db:generate": "prisma generate",
    "db:generate:safe": "prisma generate --no-engine || prisma generate",
    "db:generate:windows": "node scripts/prisma-generate-windows.js",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:reset": "prisma migrate reset",
    "build:setup": "node scripts/build-setup.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:api": "jest --testPathPattern=__tests__/api"
  },
